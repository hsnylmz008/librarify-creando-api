{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "7.4.*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.10",
        "friendsofsymfony/rest-bundle": "3.0.*",
        "gesdinet/jwt-refresh-token-bundle": "^0.12.0",
        "habbim/id-to-uuid": "dev-master",
        "league/csv": "^9.7",
        "league/flysystem-bundle": "1.5.*",
        "lexik/jwt-authentication-bundle": "^2.11",
        "ramsey/uuid-doctrine": "1.6.*",
        "sensio/framework-extra-bundle": "5.6.*",
        "sonata-project/admin-bundle": "dev-master",
        "sonata-project/doctrine-orm-admin-bundle": "dev-master",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/event-dispatcher": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/monolog-bundle": "3.6.*",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/stopwatch": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "ekino/phpstan-banned-code": "0.10.x-dev",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.68",
        "phpstan/phpstan-doctrine": "^0.12.30",
        "phpstan/phpstan-symfony": "^0.12.14",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "5.2.*",
        "symfony/debug-bundle": "5.2.*",
        "symfony/maker-bundle": "^1.18",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/var-dumper": "5.2.*",
        "symfony/web-profiler-bundle": "5.2.*",
        "symplify/easy-coding-standard": "^9.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs": [
            "vendor/bin/ecs check --ansi --no-interaction"
        ],
        "stan": [
            "vendor/bin/phpstan analyse --ansi --no-interaction"
        ],
        "cs-fix": [
            "vendor/bin/ecs check --fix --ansi --no-interaction"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
